@inherits Umbraco.Web.Macros.PartialViewMacroPage
@{
    var getApplicationsStr = Request.QueryString["getApplications"];
    bool getApplications;
}

@if (bool.TryParse(getApplicationsStr, out getApplications) && getApplications)
{
    @Html.ActionLink("Скачати заявки", "GetApplications", "WorkShopFirstDayApplication")
}
else
{
    @Html.Action("Index", "WorkShopFirstDayApplication")
}